pipeline {
    agent {label 'N1'}

    stages {
        stage('git') {
            steps {
                git 'https://github.com/praful31/studentapp-ui.git'
            }
        }
        stage('usr_to_root') {
            steps {
                sh 'sudo -i'
            }
        }
        stage('install maven') {
            steps {
                sh 'apt install maven -y'
            }
        }
    }
}
